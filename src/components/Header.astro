---
import HeaderLogo from './icons/HeaderLogo.astro';
import GithubLogo from './icons/GithubLogo.astro';
import LinkedinLogo from './icons/LinkedinLogo.astro';
---
<header class="flex justify-between items-center mt-6 w-full mx-auto px-12">
    <div>
        <nav>
            <ul class="flex gap-3">
                <li>Home</li>
                <li>Experience</li>
                <li>Portfolio</li>
                <li>About Me</li>
            </ul>
        </nav>
    </div>

    <div class="flex justify-center flex-grow">
        <HeaderLogo />
    </div>

    <div class="flex gap-2">
        <a href="#">
            <GithubLogo class="h-6 w-6" /> 
        </a>
        <a href="#">
            <LinkedinLogo class="h-6 w-6"/> 
        </a>
    </div>
</header>
